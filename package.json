  ],
  "author": "Misir Jafarov",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/TheMisir/condjs/issues"
  },
  "homepage": "https://github.com/TheMisir/condjs#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "co-exec": "^1.0",
    "codecov": "^1.0.1",
    "ghooks": "^1.2.4",
    "nyc": "^7.0.0",
    "standard": "^7.1.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run standard"
    }
